{
	"name": "SocketServer",
	"version": "0.1",
	"description": "A socket server that runs in your browser",
	"background_page": "background.html",
	"browser_action": {
		"default_icon": "graphics/c-node_small.png",
		"default_title": "Sample"
	},
	"plugins": [
		{ "path": "dll/np_node.dll"}
	],
	"permissions": [
		"http://*/*", 
		"https://*/*",
		"notifications",
		"experimental",
		"contextMenus",
		"tabs",
		"cookies"
	]
}